<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:background="@drawable/custom_management"
    tools:context="com.example.krishnaraj.collegeapp.CircularActivity">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="20dp"
        android:textStyle="bold"
        android:fontFamily="times new roman"
        android:text="Events and Activities..."
        android:id="@+id/textView3" />
   <TableRow
       android:layout_width="wrap_content"
       android:layout_height="wrap_content">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="20dp"
        android:textStyle="bold"
        android:fontFamily="times new roman"
        android:text="20.03.2016 to 31.3.2016 -- Internatioanl Conference \n\n17.3.2016 -- Technical Symposium \n\n1.4.2016 to 4.4.2016 -- Culturals \n\n5.4.2016 -- Sports day \n\n7.4.2016 -- Annual day \n\n9.4.2016 -- Achievement day \n\n20.4.2016 -- Hostel day  "
        android:layout_below="@+id/textView3"
        android:layout_alignParentStart="true"
        android:layout_marginTop="54dp" />
   </TableRow>
</RelativeLayout>
